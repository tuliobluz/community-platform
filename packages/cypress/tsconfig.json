{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "types": ["cypress", "node", "cypress-file-upload"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["**/*.ts"]
}